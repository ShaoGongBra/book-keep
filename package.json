    "expo-location": "~14.2.2",
    "expo-modules-core": "~0.9.2",
    "expo-permissions": "~13.2.0",
    "expo-sensors": "~11.3.0",
    "preact": "^10.9.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "^0.68.2",
    "react-native-bootsplash": "^4.2.3",
    "react-native-code-push": "^7.0.4",
    "react-native-device-info": "~10.0.0",
    "react-native-dux-push": "0.0.4",
    "react-native-fast-image": "^8.5.11",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "~1.10.3",
    "react-native-image-resizer": "~1.4.5",
    "react-native-pager-view": "~5.4.25",
    "react-native-safe-area-context": "~3.4.1",
    "react-native-screens": "~3.15.0",
    "react-native-share": "^7.6.4",
    "react-native-svg": "^12.3.0",
    "react-native-syan-image-picker": "0.5.3",
    "react-native-wechat-lib": "^1.1.26",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "rn-fetch-blob": "^0.12.0",
    "taro-design": "^1.0.39",
    "taro-html-view": "1.0.7",
    "taro-tools": "^1.0.22",
    "wechat-jssdk": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/runtime": "^7.18.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@prefresh/babel-plugin": "^0.4.3",
    "@tarojs/cli": "3.5.0-beta.7",
    "@tarojs/mini-runner": "3.5.0-beta.7",
    "@tarojs/rn-runner": "3.5.0-beta.7",
    "@tarojs/webpack-runner": "3.5.0-beta.7",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.17.0",
    "babel-preset-taro": "3.5.0-beta.7",
    "duxapp-cli": "^1.0.63",
    "eslint": "^8.17.0",
    "eslint-config-taro": "3.5.0-beta.7",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "install-peerdeps": "^3.0.3",
    "node-stream-zip": "^1.15.0",
    "patch-package": "^6.4.7",
    "pod-install": "^0.1.37",
    "react-refresh": "^0.14.0",
    "stylelint": "^14.9.1"
  },
  "resolutions": {
    "expo": "~45.0.6",
    "expo-av": "~11.2.3",
    "expo-barcode-scanner": "~11.3.0",
    "expo-brightness": "~10.2.0",
    "expo-camera": "~12.2.0",
    "expo-file-system": "~14.0.0",
    "expo-image-picker": "~13.1.1",
    "expo-keep-awake": "~10.1.1",
    "expo-location": "~14.2.2",
    "expo-modules-core": "~0.9.2",
    "expo-permissions": "~13.2.0",
    "expo-sensors": "~11.3.0",
    "react-native-svg": "^12.3.0"
  }
}
