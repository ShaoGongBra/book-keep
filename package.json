{
  "name": "bookkeep",
  "version": "1.0.0",
  "private": true,
  "description": "bookkeep",
  "templateInfo": {
    "name": "default",
    "typescript": false,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "duxapp rn clearBuildAssets && taro build --type rn",
    "build:rn-android": "duxapp rn clearBuildAssets android && taro build --type rn --platform android",
    "build:rn-ios": "duxapp rn clearBuildAssets ios && taro build --type rn --platform ios",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "taro build --type rn --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "postinstall": "patch-package",
    "clear:static": "duxapp project clearStatic",
    "start": "taro build --type rn --watch --reset-cache --qr",
    "codepush:init": "duxapp rn codepushInit android && duxapp rn codepushInit ios",
    "codepush:key": "duxapp rn codepushDeploymentKey android && duxapp rn codepushDeploymentKey ios",
    "android": "react-native run-android",
    "debug:android": "cd android && ./gradlew installDebug && npm run start",
    "build:android": "cd android && ./gradlew assembleRelease && cd .. && duxapp file copy android/app/build/outputs/apk/release/app-armeabi-v7a-release.apk dist/build/app-armeabi-v7a-release.apk",
    "pgyer:android": "duxapp rn pgyer dist/build/app-armeabi-v7a-release.apk",
    "install:android": "adb install android/app/build/outputs/apk/release/app-armeabi-v7a-release.apk",
    "codepush:android": "duxapp rn buildCodepushFiles android && duxapp rn codepush android",
    "codepush:android-test": "duxapp rn buildCodepushFiles android && duxapp rn codepush android dist/code-push-android Test",
    "ios": "react-native run-ios",
    "build:ios": "duxapp ios build",
    "export:ios": "duxapp ios export",
    "pgyer:ios": "duxapp rn pgyer dist/ios/duxapp.ipa/duxapp.ipa",
    "upload-ios": "duxapp ios upload dist/ios/duxapp.ipa/duxapp.ipa",
    "codepush:ios": "duxapp rn buildCodepushFiles ios && duxapp rn codepush ios",
    "codepush:ios-test": "duxapp rn buildCodepushFiles ios && duxapp rn codepush ios dist/code-push-ios Test",
    "codepush": "npm run build:rn-android && npm run codepush:android && npm run build:rn-ios && npm run codepush:ios",
    "codepush-test": "npm run build:rn-android && npm run codepush:android-test && npm run build:rn-ios && npm run codepush:ios-test",
    "test": "jest",
    "lint": "eslint ."
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@0x5e/react-native-alipay": "^0.2.7-beta.2",
    "@react-native-async-storage/async-storage": "~1.17.6",
    "@react-native-community/cameraroll": "~4.1.2",
    "@react-native-community/clipboard": "~1.5.1",
    "@react-native-community/geolocation": "~2.1.0",
    "@react-native-community/netinfo": "~9.0.0",
    "@react-native-community/push-notification-ios": "^1.10.1",
    "@react-native-community/slider": "~4.2.3",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-native-picker/picker": "~2.4.1",
    "@tarojs/components": "3.5.0-beta.7",
    "@tarojs/plugin-framework-react": "3.5.0-beta.7",
    "@tarojs/react": "3.5.0-beta.7",
    "@tarojs/runtime": "3.5.0-beta.7",
    "@tarojs/taro": "3.5.0-beta.7",
    "@tarojs/taro-rn": "3.5.0-beta.7",
    "crypto-js": "^4.1.1",
    "expo": "~45.0.6",
    "expo-av": "~11.2.3",
    "expo-barcode-scanner": "~11.3.0",
    "expo-brightness": "~10.2.0",
    "expo-camera": "~12.2.0",
    "expo-file-system": "~14.0.0",
    "expo-image-picker": "~13.1.1",
